{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/email-import-help",
      "dest": "/server.js"
    },
    {
      "src": "/reset-password",
      "dest": "/server.js"
    },
    {
      "src": "/forgot-password",
      "dest": "/public/forgot-password.html"
    },
    {
      "src": "/forgot-password-sent",
      "dest": "/public/forgot-password-sent.html"
    },
    {
      "src": "/admin/login",
      "dest": "/public/admin-login.html"
    },
    {
      "src": "/admin2/login",
      "dest": "/public/admin2-login.html"
    },
    {
      "src": "/login",
      "dest": "/public/admin-login.html"
    },
    {
      "src": "/app",
      "dest": "/public/index.html"
    },
    {
      "src": "/welcome",
      "dest": "/public/welcome.html"
    },
    {
      "src": "/subscription-confirm",
      "dest": "/public/subscription-confirm.html"
    },
    {
      "src": "/beta-expired",
      "dest": "/public/beta-expired.html"
    },
    {
      "src": "/register(.html)?",
      "dest": "/public/waitlist.html"
    },
    {
      "src": "/(.*\\.(js|css|html|png|jpg|jpeg|gif|svg|ico))",
      "dest": "/public/$1"
    },
    {
      "src": "/",
      "dest": "/public/waitlist.html"
    }
  ]
}